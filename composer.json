{
    "name": "meyfa/php-svg",
    "homepage": "https://github.com/meyfa/php-svg",
    "type": "library",
    "description": "Read, edit, write, and render SVG files with PHP",
    "keywords": ["svg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian Meyer",
            "homepage": "http://meyfa.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "SVG\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SVG\\": "tests/"
        }
    }
}
